{
  "env": {
    "development": {
      "plugins": [
      	"transform-export-extensions",
        "add-module-exports",
        "transform-object-rest-spread",
        "syntax-flow",
        "tcomb",
        "transform-flow-strip-types",
        "istanbul"
      ]
    },
    "production": {
      "plugins": [
      	"transform-export-extensions",
        "add-module-exports",
        "transform-object-rest-spread",
        "syntax-flow",
        "transform-flow-strip-types"
      ]
    }
  },
  "presets": [
    "es2015-node4"
  ]
}
